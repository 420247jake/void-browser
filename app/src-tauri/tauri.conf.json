{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Void Browser",
  "version": "1.1.0",
  "identifier": "com.jacobterrell.voidbrowser",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Void Browser",
        "width": 1400,
        "height": 900,
        "minWidth": 800,
        "minHeight": 600,
        "resizable": true,
        "fullscreen": false,
        "decorations": true,
        "transparent": false,
        "center": true
      }
    ],
    "security": {
      "csp": "default-src 'self'; script-src 'self' 'unsafe-inline' 'unsafe-eval' blob:; style-src 'self' 'unsafe-inline'; img-src 'self' data: blob: https:; font-src 'self' data:; connect-src 'self' https: wss: ws:; media-src 'self' blob:; frame-src 'none'"
    }
  },
  "bundle": {
    "active": true,
    "targets": ["msi", "nsis"],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [],
    "copyright": "Â© 2026 Jacob Terrell",
    "category": "Utility",
    "shortDescription": "3D spatial web browser - fly through the internet",
    "longDescription": "Void Browser transforms your web browsing history into a stunning 3D universe. Fly through interconnected nodes representing websites, discover link relationships, and visualize your digital footprint like never before.",
    "publisher": "Jacob Terrell",
    "homepage": "https://jacobterrell.dev/void-browser",
    "windows": {
      "digestAlgorithm": "sha256",
      "timestampUrl": "http://timestamp.digicert.com",
      "allowDowngrades": true,
      "nsis": {
        "installerIcon": "icons/icon.ico",
        "installMode": "currentUser",
        "languages": ["English"],
        "displayLanguageSelector": false
      }
    }
  },
  "plugins": {
    "sql": {
      "preload": []
    }
  }
}